  },
  "homepage": "https://github.com/techstack-nz/reselect-lens#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
